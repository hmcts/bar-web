{
  "name": "bar-web",
  "version": "1.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.5.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "node start.js",
    "start:angular-dev": "ng serve --proxy-config proxy.conf.json",
    "start:express-dev": "cross-env NODE_ENV=development IGNORE_CERTS=true nodemon --inspect start.js",
    "start:express-prod": "cross-env nodemon --inspect start.js",
    "start-selenium": "selenium-standalone start",
    "build": "ng build --prod && yarn pugit dist/index.html --output express/mvc/views/index.pug && rm dist/index.html",
    "postbuild": "yarn replace file=express/mvc/views/index.pug searchValue='\\\"csrfToken\\\"' newValue='csrfToken'",
    "ng-lint": "ng lint",
    "eslint": "eslint .",
    "lint": "yarn delete:output-folders && yarn ng-lint && yarn eslint",
    "e2e": "ng e2e",
    "lint:fix": "yarn eslint --fix && yarn ng lint --fix",
    "setup": "echo 'setup not needed'",
    "gulp": "gulp",
    "ng:build": "yarn build",
    "pre-push": "yarn test-coverage-report",
    "security-checks": "yarn test:nsp",
    "test": "yarn lint && yarn test:nsp && yarn test:express && yarn test:angular && yarn test:coverage",
    "test:a11y": "cross-env node scripts/pa11y",
    "test:angular": "ng test --watch=false --code-coverage && yarn rename source=./coverage/lcov.info destination=./coverage/lcov-angular.info",
    "test:express": "cross-env NODE_ENV=development nyc --reporter=lcov --reporter=text mocha ./express/**/*.test.js --recursive && yarn rename source=./coverage/lcov.info destination=./coverage/lcov-express.info",
    "test-e2e": "cross-env NODE_CONFIG_DIR=e2e/config codeceptjs run -c e2e --steps",
    "test:functional": "./bin/run-functional-tests.sh",
    "test:acceptance": "cross-env LOG_LEVEL=OFF NODE_PATH=. ./node_modules/.bin/codeceptjs run -c acceptance-tests/codecept.conf.js --reporter mochawesome --bail",
    "test:acceptance-local": "cross-env LOG_LEVEL=OFF NODE_PATH=. ./node_modules/.bin/codeceptjs run -c acceptance-tests/codecept-local.conf.js --reporter mochawesome --bail",
    "test:functional-local": "./bin/run-functional-tests-local.sh",
    "test:nsp": "nsp check",
    "test:smoke": "codeceptjs run -c smoke-test/smoke-codecept.conf.js --reporter mochawesome --bail",
    "copy-lcov": "node ./scripts/copy-lcov.js",
    "rename": "node ./scripts/rename.js",
    "replace": "node ./scripts/replace.js",
    "merge-lcov": "cross-env lcov-result-merger './coverage/lcov-*.info' './coverage/lcov.info'",
    "test:coverage": "yarn merge-lcov && yarn copy-lcov",
    "sonar-scan": "sonar-scanner",
    "delete:output-folders": "rm -rf output/ && rm -rf acceptance-tests/output/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@hmcts/nodejs-logging": "^3.0.0",
    "@types/lodash": "4.14.108",
    "applicationinsights": "^1.0.5",
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "chai-as-promised": "^7.1.1",
    "client-request": "^2.3.0",
    "config": "^1.28.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "csurf": "^1.9.0",
    "debug": "~2.6.0",
    "express": "^4.16.2",
    "express-validator": "^3.1.2",
    "hbs": "~4.0.1",
    "helmet": "^3.9.0",
    "http-status-codes": "^1.3.0",
    "i18n": "^0.8.3",
    "install": "^0.12.1",
    "js-money": "^0.6.3",
    "js-yaml": "^3.10.0",
    "jsdom": "^9.12.0",
    "lodash": "4.17.10",
    "log4js": "^1.1.0",
    "mocha-junit-reporter": "^1.17.0",
    "mochawesome": "^3.0.2",
    "moment": "2.22.2",
    "moment-timezone": "^0.5.21",
    "morgan": "~1.9.0",
    "ngx-cookie-service": "^1.0.10",
    "nsp": "^3.2.1",
    "pa11y": "^5.0.4",
    "pug": "2.0.3",
    "require-directory": "^2.1.1",
    "rxjs": "^5.4.2",
    "serve-favicon": "~2.5.0",
    "superagent": "^3.6.0",
    "supertest": "^3.0.0",
    "uuid": "3.2.1",
    "validator": "^10.0.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@hmcts/eslint-config": "^1.3.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "chai": "4.1.2",
    "chai-http": "4.0.0",
    "codeceptjs": "^1.3.1",
    "codelyzer": "~3.2.0",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.12.1",
    "eslint-plugin-node": "^5.2.1",
    "faker": "^4.1.0",
    "govuk-elements-sass": "^3.1.2",
    "govuk_frontend_toolkit": "^7.1.0",
    "gulp": "^3.9.1",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-intl-shim": "1.0.3",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "1.0.4",
    "lcov-result-merger": "3.1.0",
    "mocha": "^4.0.1",
    "nock": "^9.1.4",
    "nodemon": "^1.12.1",
    "nyc": "^11.7.1",
    "pa11y-reporter-html": "^1.0.0",
    "phantomjs-prebuilt": "2.1.16",
    "protractor": "^5.3.2",
    "proxyquire": "^1.7.11",
    "pugit": "6.1.1",
    "puppeteer": "^1.5.0",
    "rosie": "^1.6.0",
    "selenium-standalone": "^6.14.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.13.0",
    "sonar-scanner": "^3.0.3",
    "ts-mockito": "2.3.0",
    "tslint": "~5.7.0",
    "typescript": "~2.3.3",
    "webdriverio": "^4.9.11"
  }
}
