  "engines": {
    "node": ">=v18.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "node start.js",
    "start-dev": "NODE_ENV=development node start.js",
    "start:angular-dev": "ng serve --proxy-config proxy.conf.json",
    "start:express-dev": "cross-env NODE_ENV=development IGNORE_CERTS=true nodemon --inspect start.js",
    "start:express-prod": "cross-env IGNORE_CERTS=true nodemon --inspect start.js",
    "start-selenium": "selenium-standalone start",
    "build": "ng build --configuration production && yarn pugit dist/index.html --output express/mvc/views/index.pug && rm dist/index.html && yarn postbuild",
    "postbuild": "yarn replace file=express/mvc/views/index.pug searchValue='\\\"csrfToken\\\"' newValue='csrfToken'",
    "lint": "stylelint **/*.scss -v -q",
    "lint:fix": "prettier --write . .github && eslint --fix .",
    "e2e": "ng e2e",
    "setup": "echo 'setup not needed'",
    "ng:build": "yarn build",
    "pre-push": "yarn test-coverage-report",
    "test": "yarn test:angular && yarn test:coverage",
    "test:a11y": "cross-env node scripts/pa11y",
    "test:angular": "ng test --watch=false --code-coverage",
    "test:express": "cross-env NODE_ENV=development nyc --reporter=lcov --reporter=text mocha ./express/**/*.test.js --recursive && yarn rename source=./coverage/lcov.info destination=./coverage/lcov-express.info",
    "fortifyScan": "./src/test/java/gradlew -p src/test/java fortifyScan",
    "copy-lcov": "node ./scripts/copy-lcov.js",
    "rename": "node ./scripts/rename.js",
    "replace": "node ./scripts/replace.js",
    "merge-lcov": "cross-env lcov-result-merger './coverage/lcov-*.info' './coverage/lcov.info'",
    "test:coverage": "yarn merge-lcov && yarn copy-lcov",
    "sonar-scan": "sonar-scanner",
    "delete:output-folders": "rm -rf output/ && rm -rf acceptance-tests/output/",
    "test:smoke": "codeceptjs run --config smoke-test/smoke-codecept.conf.js",
    "test:functional": "npx playwright install chromium && npm-run-all -scl test:acceptance test:functional:report",
    "test:fullfunctional": "npm-run-all -scl test:acceptance test:functional:report",
    "test:acceptance": "npx playwright install chromium && ./node_modules/.bin/codeceptjs run-workers 6 parallel --config acceptance-tests/codecept.conf.js --grep @functional --verbose",
    "test:functional:report": "allure generate functional-output/functional/reports -c -o functional-output/functional/allure",
    "test:crossbrowser": "npm-run-all -scl test:crossbrowser:e2e test:crossbrowser:report",
    "test:crossbrowser:e2e": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run-multiple --all --config acceptance-tests/saucelabs.conf.js --grep @crossbrowser",
    "test:crossbrowser:report": "allure generate functional-output/cross-browser/reports/* -c -o functional-output/cross-browser/allure"
  },
